pipeline {
    agent any

    stages {
        stage('git-clone') {
            steps {
               git branch: 'main', changelog: false, poll: false, url: 'https://github.com/mohammedali6969/demo.git'
            }
        }
 stage('run') {
            steps {
               sh 'python3 app.py'
            }
        }
}
}
